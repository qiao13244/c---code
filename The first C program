int main()
{
	int num1 = 0;
	int num2 = 0;
	//输入数据-使用输入函数scanf
	scanf("%d%d",&num1,&num2);//取地址符号&
	int num3 = num1 + num2;
	printf("%d",num3);

	return 0;

}
